    height: 87rpx;
    line-height: 87rpx;
    color:#1f65d8;
    font-size: 28rpx;
}
.invoice-item{
    display:flex;
    padding-bottom: 10rpx;
}
.invoice-info{
    flex: 1;
    display:flex;
    justify-content: space-between;
    font-size: 24rpx;
}
.invoice-left-wrapper{
    padding-left: 20rpx;
}
.invoice-title{
    padding-bottom: 10rpx;
}
.invoice-right{
    font-size: 30rpx;
    font-weight:bold;
}
.invoice-tag{
    display:inline-block;
    padding: 10rpx;
    font-size: 20rpx;
    border: 1px solid #ff5252;
    border-radius: 4rpx;
}
.delete-btn{
    width: 40rpx;
    height: 40rpx;
    background:url(data:image/png;base64,) center center no-repeat;
    background-size: 40rpx 40rpx;
}
