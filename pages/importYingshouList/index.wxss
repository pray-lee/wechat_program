.add-borrow-page{
    height: 100%;
    transform: translateY(0)
}
.jiekuandan {
    height: calc(100vh - 400rpx)
}
.jiekuandan.iphoneX{
    height: calc(100vh - 468rpx)
}
.import-borrow-item{
    flex: 1
}
.import-borrow-item .form-row{
    justify-content: space-between;
    font-size: 24rpx;
    padding: 0;
}
.import-borrow-item .form-line{
    margin-left: 0
}
.import-borrow-remark {
    line-height: 30rpx;
    margin-top: 10rpx;
}
.import-borrow-info{
    display:flex;
    align-items: center;
    color:#b8b9bb;
    font-weight: 300;
    font-size: 22rpx
}
.checkbox-label{
    display:flex;
    align-items: center;
    padding: 0 15rpx;
}
.checkbox{
    margin-right: 15rpx;
    border-radius: 50%;
}
.import-borrow-list-btn{
}
.import-borrow-input{
    font-size: 24rpx
}
.btn-area{
    display:flex;
    position:fixed;
    bottom:0;
    left:0;
    z-index:990;
    width: 100%;
}
.btn-area button{
    flex: 1;
    border:0;
    background:#3276c3;
    border-radius: 0;
    color:#fff;
    font-size: 32rpx;
    height: 100rpx;
    line-height: 100rpx
}
.search-wrapper{
    position:relative;
    background:#f1f1f1;
    padding: 25rpx;
}
.search-wrapper input{
    box-sizing:border-box;
    width: 100%;
    height: 100%;
    font-size: 28rpx;
    border-radius: 4px;
    padding: 24rpx;
    background:#fff;
}
.date-wrapper{
    height: 80rpx;
    display:flex;
    justify-content: space-between;
    padding: 0 25rpx;
    background:#f1f1f1
}
.start, .end{
    flex: 1;
    position:relative;
    display:flex;
    align-items: center;
}
.date-icon{
    width:34rpx;
    height:31.2rpx;
    position:static;
    margin-right: 20rpx;
}
.selectAll-wrapper{
    display:flex;
    justify-content: space-between;
    align-items:center;
    color:#3276c3a;
    font-size: 26rpx;
    padding: 20rpx 14rpx
}
.selectAll{
    padding: 15rpx;
    padding-bottom: 0
}
.total-wrapper{
    padding-right: 15rpx;
}
.select-date{
    font-size: 25rpx
}
.del-date{
    color:#999;
    font-size: 25rpx;
    margin-left: 10rpx;
}
